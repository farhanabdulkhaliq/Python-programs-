import random

name = input('Enter your name: ').lower()
age = int(input('Enter your age: '))

if age < 18:
    print('You have to be 18+ to register')
else:
    registration_number = random.randint(10000,99999)
    print(f'Your registration numbers is {registration_number}')
    print(f'        Name - {name}')
    print(f'        Age - {age}')
    print(f'        Registration_number - {registration_number}')
